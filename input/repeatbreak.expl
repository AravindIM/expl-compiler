begin
    decl
        int a, b;
    enddecl
    a = 20;
    repeat
        read(b);
        if ( b == 0 ) then
            break;
        endif;
        if ( b == 1 ) then
            continue;
        endif;
        if ( b > a ) then
            write("lower it");
        else
            write("raise it");
        endif;
    until ( b == a );
    write("equal");
end