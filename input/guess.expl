begin
    decl
        int correct_answer, your_guess;
    enddecl
    correct_answer = 20;
    read(your_guess);
    while( your_guess != correct_answer ) do
        if ( your_guess < correct_answer ) then
            write("less");
        else
            write("greater");
        endif;
        read(your_guess);
    endwhile;
    write("equal");
end